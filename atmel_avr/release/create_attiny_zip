rm -rf examples
rm -rf lib
rm dogm128_avr.zip
cp -R ../../libraries/Dogm/utility .
mv utility lib
cp -R ../attiny84/examples .
cp ../../COPYING.LESSER .
cp ../attiny84/INSTALL.TXT .
cp ../../ChangeLog .
find . -name Makefile -exec sed -iold -e "s|DOGDIR = .*|DOGDIR = ../../lib/|" {}  \;  
find . -name "Makefile" -execdir make clean \;
zip -r dogm128_avr.zip examples lib COPYING.LESSER INSTALL.TXT ChangeLog
zip -d dogm128_avr.zip "*/Makefileold"